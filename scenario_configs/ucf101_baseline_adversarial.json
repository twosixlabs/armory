{
    "_description": "UCF101 video classification from pretrained, contributed by MITRE Corporation",
    "adhoc": null,
    "attack": {
        "knowledge": "white",
        "kwargs": {
            "adversarial_key": "adversarial_patch",
            "batch_size": 1,
            "description": "'adversarial_key' can be 'adversarial_perturbation' or 'adversarial_patch'"
        },
        "module": "armory.data.adversarial_datasets",
        "name": "ucf101_adversarial_112x112",
        "type": "preloaded",
        "use_label": false
    },
    "dataset": {
        "batch_size": 16,
        "framework": "numpy",
        "module": "armory.data.datasets",
        "name": "ucf101"
    },
    "defense": {
        "kwargs": {
            "apply_fit": false,
            "apply_predict": true,
            "clip_values": [
                0.0,
                255.0
            ],
            "means": [
                114.7748,
                107.7354,
                99.475
            ],
            "quality": 50,
            "transpose": [
                1,
                2,
                3,
                0
            ]
        },
        "module": "armory.art_experimental.defences.jpeg_compression_5d",
        "name": "JpegCompression5D",
        "type": "Preprocessor"
    },
    "metric": {
        "means": true,
        "perturbation": "linf",
        "record_metric_per_sample": false,
        "task": [
            "categorical_accuracy",
            "top_5_categorical_accuracy"
        ]
    },
    "model": {
        "fit": false,
        "fit_kwargs": {
            "nb_epochs": 10
        },
        "model_kwargs": {
            "model_status": "ucf101_trained"
        },
        "module": "armory.baseline_models.pytorch.ucf101_mars",
        "name": "get_art_model",
        "weights_file": "mars_ucf101_v1.pth",
        "wrapper_kwargs": {}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.video_ucf101_scenario",
        "name": "Ucf101"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/pytorch:0.8.0",
        "external_github_repo": "yusong-tan/MARS",
        "gpus": "all",
        "output_dir": null,
        "output_filename": null,
        "use_gpu": false
    }
}
